---
import type {IconProps} from '@/types';

const {
  size = 24,
  fill = 'none',
  stroke = 'currentColor',
  class: className,
  ...props
} = Astro.props as IconProps;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={size}
  height={size}
  viewBox="0 0 24 24"
  fill={fill}
  stroke={stroke}
  stroke-width="2.5"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
  {...props}
>
