---
import type {IconProps} from '@/types';

const {
  width = '24',
  height = '24',
  fill = 'none',
  stroke = 'currentColor',
  class: className,
  id,
} = Astro.props as IconProps;
---

<svg
  class={className}
  xmlns="http://www.w3.org/2000/svg"
  width={width}
  height={height}
  viewBox="0 0 24 24"
  fill={fill}
  stroke={stroke}
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  id={id}
>
  <path d="M21 12.79A9 9 0 1 1 11.21 3 7 7 0 0 0 21 12.79z"></path>
</svg>
