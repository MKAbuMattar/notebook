---
import type {IconProps} from '@/types';

const {
  width = '24',
  height = '24',
  fill = 'none',
  stroke = 'currentColor',
  class: className,
  id,
} = Astro.props as IconProps;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={width}
  height={height}
  viewBox="0 0 24 24"
  fill={fill}
  stroke={stroke}
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
  id={id}
>
  <path d="M18 15l-6-6-6 6"></path>
</svg>
