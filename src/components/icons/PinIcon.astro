---
import type {IconProps} from '@/types';

const {
  class: className,
  size = 24,
  fill = 'none',
} = Astro.props as IconProps & {fill?: string};
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={size}
  height={size}
  viewBox="0 0 24 24"
  fill={fill}
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
>
  <path d="M12 2L4.5 20.29l.71.71L12 18l6.79 3 .71-.71z"></path>
</svg>
